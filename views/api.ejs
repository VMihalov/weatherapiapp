
<div class="row">
	<% for(var i = 0; i < 7; i++) { %>
	<div class="col">
		<div class="card">
			<div class="card-title">
				<b><%=week[i] %></b>
			</div>
			<div class="card-image">
				<img src='http://openweathermap.org/img/wn/<%=api.list[i].weather[0].icon  %>@2x.png'>
			</div>
		</div>
	</div>
	<% } %>
</div>
	
